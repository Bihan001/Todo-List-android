{
  "expo": {
    "name": "Todo List",
    "slug": "bh-todo-app",
    "version": "1.0.0",
    "sdkVersion": "38.0.0",
    "orientation": "portrait",
    "icon": "./assets/todo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/todo.png"
    },
    "android": {
      "package": "com.bihan.todo",
      "versionCode": 1,
      "permissions": [],
      "icon": "./assets/todo.png",
      "enableDangerousExperimentalLeanBuilds": true
    },
    "description": "Todo List to keep track of your future plans and activities."
  }
}
